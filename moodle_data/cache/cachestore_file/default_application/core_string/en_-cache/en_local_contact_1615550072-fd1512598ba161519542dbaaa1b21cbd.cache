a:28:{s:10:"pluginname";s:12:"Contact Form";s:16:"privacy:metadata";s:72:"The Contact Form plugin does not store any personal data about any user.";s:10:"globalhelp";s:150:"Contact Form is a plugin for Moodle that allows your site to process information submitted through HTML web forms to the site's support email address.";s:9:"configure";s:21:"Configure this plugin";s:10:"field-name";s:4:"name";s:11:"field-email";s:5:"email";s:13:"field-subject";s:7:"subject";s:13:"field-message";s:7:"message";s:19:"confirmationmessage";s:81:"Thank you for contacting us. If required, we will be in touch with you very soon.";s:16:"confirmationsent";s:47:"An email has been sent to your address at {$a}.";s:9:"forbidden";s:9:"Forbidden";s:17:"errorsendingtitle";s:21:"Failed to send e-mail";s:12:"errorsending";s:68:"An error occurred while sending the message. Please try again later.";s:14:"defaultsubject";s:11:"New message";s:9:"extrainfo";s:380:"<hr>
